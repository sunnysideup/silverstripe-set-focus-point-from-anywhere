{
  "name": "sunnysideup/set-focus-point-from-anywhere",
  "description": "React or not react - this provides a FormField that allows you to set the focus point of an image",
  "type": "silverstripe-vendormodule",
  "keywords": [
    "jonom",
    "focus point",
    "focuspoint",
    "react",
    "non-react",
    "form field",
    "image",
    "image focus point",
    "nice cropping",
    "crop better"
  ],
  "license": "BSD-3-Clause",
  "require": {
    "silverstripe/framework": "^5.0",
    "silverstripe/admin": "^2.0"
  },
  "require-dev": {
    "sunnysideup/sswebpack_engine_only": "dev-master",
    "silverleague/ideannotator": "*",
    "phpunit/phpunit": "^9.6",
    "squizlabs/php_codesniffer": "^3.7",
    "silverstripe/standards": "^1",
    "phpstan/extension-installer": "^1.3"
  },
  "autoload": {
    "psr-4": {
      "Sunnysideup\\SetFocusPointFromAnywhere\\": "src/"
    }
  },
  "extra": {
    "expose": [
      "client/dist"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "bin": [
    "bin/sake-npm-build-set-focus-point-from-anywhere.sh",
    "bin/sake-npm-watch-set-focus-point-from-anywhere.sh"
  ],
  "authors": [
    {
      "name": "Nicolaas Thiemen Francken",
      "email": "modules@sunnysideup.co.nz",
      "role": "lead developer"
    }
  ]
}
